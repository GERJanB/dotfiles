    "explorer.confirmDelete": false,
    "editor.rulers": [120],
    "cSpell.language": "de-de,en",
    "workbench.editor.showTabs": "none",
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/bin/nvim",
    "vim.insertModeKeyBindings": [
        {
            "before": ["รถ", "รถ"],
            "after": ["<Esc>"]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": ["รถ", "รถ"],
            "after": ["<Esc>"]
        }
    ]
}